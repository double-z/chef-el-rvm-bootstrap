# Dynamically generated by Chef for <%= node['fqdn'] %> -
# local modifications will be replaced.
#
# Cookbook: <%= @cookbook_name %>
# Recipe: <%= @recipe_name %>
#
<%= node['chef_client']['log_dir'] %>/client.log {
  rotate 12
  weekly
  compress
  postrotate
    /etc/init.d/chef-client condrestart >/dev/null || :
  endscript
}
