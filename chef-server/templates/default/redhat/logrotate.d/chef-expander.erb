# Dynamically generated by Chef for <%= node['fqdn'] %> -
# local modifications will be replaced.
#
# Cookbook: <%= @cookbook_name %>
# Recipe: <%= @recipe_name %>
#
/var/log/chef/expander.log {
  rotate 12
  weekly
  compress
  postrotate
    /etc/init.d/chef-expander condrestart >/dev/null || :
  endscript
}
